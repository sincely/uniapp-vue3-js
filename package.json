    "build:quickapp-webview": "uni build -p quickapp-webview",
    "build:quickapp-webview-huawei": "uni build -p quickapp-webview-huawei",
    "build:quickapp-webview-union": "uni build -p quickapp-webview-union",
    "preinstall": "npx only-allow pnpm",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "lint": "eslint --ext .js,.vue --fix ./src",
    "eslint": "eslint \"src/**/*.{js,jsx,ts,tsx,vue}\"",
    "eslint:fix": "eslint \"src/**/*.{js,jsx,ts,vue}\" --fix",
    "stylelint": "stylelint \"src/**/*.{vue,scss,css,sass,less}\"",
    "stylelint:fix": "stylelint \"src/**/*.{vue,scss,css,sass,less}\" --fix",
    "prettier:fix": "prettier --write \"src/**/*.{js,vue,scss,css,json,html}\"",
    "manual:release": "node scripts/release.js",
    "release": "release-it",
    "release:dry-run": "release-it --dry-run",
    "release:changelog": "release-it --only-changelog",
    "release:github": "release-it --ci --github.release",
    "release:patch": "release-it patch",
    "release:minor": "release-it minor",
    "release:major": "release-it major",
    "release:alpha": "release-it --preRelease=alpha",
    "release:beta": "release-it --preRelease=beta"
  },
